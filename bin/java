#!/bin/sh

JCP=""
SEP=";"
EXE=java
while read line || [ -n "$line" ] ; do
  case $line in
    '#'*)
      ;;
    '!'*)
      JCP=$SEP${line#"!"}$JCP
      ;;
    *) 
      JCP=$JCP$SEP$line
      ;;
  esac
done < class.jcp
JCP=${JCP#$SEP}

if [ "" = "$JAVA_HOME" ] ; then
  EXE=$(which -a $EXE | grep -v "$0" | head -1)
else 
  EXE=$JAVA_HOME/bin/$EXE
fi
$EXE -cp "$JCP" "$@"